{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"mifosx-lib": {
			"projectType": "library",
			"root": "projects/mifosx-lib",
			"sourceRoot": "projects/mifosx-lib/src",
			"prefix": "lib",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:ng-packagr",
					"options": {
						"project": "projects/mifosx-lib/ng-package.json"
					},
					"configurations": {
						"production": {
							"tsConfig": "projects/mifosx-lib/tsconfig.lib.prod.json"
						},
						"development": {
							"tsConfig": "projects/mifosx-lib/tsconfig.lib.json"
						}
					},
					"defaultConfiguration": "production"
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "projects/mifosx-lib/src/test.ts",
						"tsConfig": "projects/mifosx-lib/tsconfig.spec.json",
						"karmaConfig": "projects/mifosx-lib/karma.conf.js"
					}
				}
			}
		},
		"mifosx-app": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				}
			},
			"root": "projects/mifosx-app",
			"sourceRoot": "projects/mifosx-app/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "ngx-build-plus:browser",
					"options": {
						"outputPath": "dist/mifosx-app",
						"index": "projects/mifosx-app/src/index.html",
						"main": "projects/mifosx-app/src/main.ts",
						"polyfills": "projects/mifosx-app/src/polyfills.ts",
						"tsConfig": "projects/mifosx-app/tsconfig.app.json",
						"inlineStyleLanguage": "scss",
						"stylePreprocessorOptions": {
							"includePaths": [
								"projects/mifosx-lib/src"
							]
						},
						"assets": [
							"projects/mifosx-app/src/favicon.ico",
							"projects/mifosx-app/src/assets"
						],
						"styles": [
							"projects/mifosx-app/src/styles.scss"
						],
						"scripts": [],
						"extraWebpackConfig": "projects/mifosx-app/webpack.config.js",
						"commonChunk": false
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kb",
									"maximumError": "1mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "100kb"
								}
							],
							"fileReplacements": [
								{
									"replace": "projects/mifosx-app/src/environments/environment.ts",
									"with": "projects/mifosx-app/src/environments/environment.prod.ts"
								}
							],
							"outputHashing": "all",
							"extraWebpackConfig": "projects/mifosx-app/webpack.prod.config.js"
						},
						"development": {
							"buildOptimizer": false,
							"optimization": false,
							"vendorChunk": true,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "ngx-build-plus:dev-server",
					"configurations": {
						"production": {
							"browserTarget": "mifosx-app:build:production",
							"extraWebpackConfig": "projects/mifosx-app/webpack.prod.config.js"
						},
						"development": {
							"browserTarget": "mifosx-app:build:development"
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"port": 4202,
						"publicHost": "http://localhost:4202",
						"extraWebpackConfig": "projects/mifosx-app/webpack.config.js"
					}
				},
				"extract-i18n": {
					"builder": "ngx-build-plus:extract-i18n",
					"options": {
						"browserTarget": "mifosx-app:build",
						"extraWebpackConfig": "projects/mifosx-app/webpack.config.js"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "projects/mifosx-app/src/test.ts",
						"polyfills": "projects/mifosx-app/src/polyfills.ts",
						"tsConfig": "projects/mifosx-app/tsconfig.spec.json",
						"karmaConfig": "projects/mifosx-app/karma.conf.js",
						"inlineStyleLanguage": "scss",
						"assets": [
							"projects/mifosx-app/src/favicon.ico",
							"projects/mifosx-app/src/assets"
						],
						"styles": [
							"projects/mifosx-app/src/styles.scss"
						],
						"scripts": []
					}
				}
			}
		},
		"fineract-client": {
			"projectType": "library",
			"root": "projects/fineract-client",
			"sourceRoot": "projects/fineract-client/src",
			"prefix": "lib",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:ng-packagr",
					"options": {
						"project": "projects/fineract-client/ng-package.json"
					},
					"configurations": {
						"production": {
							"tsConfig": "projects/fineract-client/tsconfig.lib.prod.json"
						},
						"development": {
							"tsConfig": "projects/fineract-client/tsconfig.lib.json"
						}
					},
					"defaultConfiguration": "production"
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "projects/fineract-client/src/test.ts",
						"tsConfig": "projects/fineract-client/tsconfig.spec.json",
						"karmaConfig": "projects/fineract-client/karma.conf.js"
					}
				}
			}
		},
		"mifosx-shell": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				}
			},
			"root": "projects/mifosx-shell",
			"sourceRoot": "projects/mifosx-shell/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "ngx-build-plus:browser",
					"options": {
						"outputPath": "dist/mifosx-shell",
						"index": "projects/mifosx-shell/src/index.html",
						"main": "projects/mifosx-shell/src/main.ts",
						"polyfills": "projects/mifosx-shell/src/polyfills.ts",
						"tsConfig": "projects/mifosx-shell/tsconfig.app.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							"projects/mifosx-shell/src/favicon.ico",
							"projects/mifosx-shell/src/assets"
						],
						"styles": [
							"projects/mifosx-shell/src/styles.scss"
						],
						"scripts": [],
						"extraWebpackConfig": "projects/mifosx-shell/webpack.config.js",
						"commonChunk": false
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kb",
									"maximumError": "1mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "4kb"
								}
							],
							"fileReplacements": [
								{
									"replace": "projects/mifosx-shell/src/environments/environment.ts",
									"with": "projects/mifosx-shell/src/environments/environment.prod.ts"
								}
							],
							"outputHashing": "all",
							"extraWebpackConfig": "projects/mifosx-shell/webpack.prod.config.js"
						},
						"development": {
							"buildOptimizer": false,
							"optimization": false,
							"vendorChunk": true,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "ngx-build-plus:dev-server",
					"configurations": {
						"production": {
							"browserTarget": "mifosx-shell:build:production",
							"extraWebpackConfig": "projects/mifosx-shell/webpack.prod.config.js"
						},
						"development": {
							"browserTarget": "mifosx-shell:build:development"
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"port": 4200,
						"publicHost": "http://localhost:4200",
						"extraWebpackConfig": "projects/mifosx-shell/webpack.config.js"
					}
				},
				"extract-i18n": {
					"builder": "ngx-build-plus:extract-i18n",
					"options": {
						"browserTarget": "mifosx-shell:build",
						"extraWebpackConfig": "projects/mifosx-shell/webpack.config.js"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "projects/mifosx-shell/src/test.ts",
						"polyfills": "projects/mifosx-shell/src/polyfills.ts",
						"tsConfig": "projects/mifosx-shell/tsconfig.spec.json",
						"karmaConfig": "projects/mifosx-shell/karma.conf.js",
						"inlineStyleLanguage": "scss",
						"assets": [
							"projects/mifosx-shell/src/favicon.ico",
							"projects/mifosx-shell/src/assets"
						],
						"styles": [
							"projects/mifosx-shell/src/styles.scss"
						],
						"scripts": []
					}
				}
			}
		}
	},
	"cli": {
		"analytics": "fb2cd6e7-245c-4b1b-901a-10fd9a7a803d"
	}
}